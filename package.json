{
  "name": "s3-unzipper",
  "version": "1.0.0",
  "description": "AWS Lambda function to process zip files from S3 and organize extracted files by stem names",
  "main": "dist/handlers/s3-unzipper.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --write src tests",
    "format": "biome format src tests --write",
    "format:check": "biome format src tests",
    "check": "biome check src tests",
    "check:fix": "biome check --write src tests",
    "prepare": "husky install",
    "sam:build": "sam build",
    "sam:local": "sam local invoke UnzipperFunction -e events/s3-event.json",
    "deploy": "npm run build && sam build && sam deploy",
    "deploy:guided": "npm run build && sam build && sam deploy --guided",
    "ci": "npm run check && npm run test:coverage && npm run build"
  },
  "keywords": [
    "aws",
    "lambda",
    "s3",
    "zip",
    "serverless",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.3",
    "@aws-sdk/client-s3": "^3.485.0",
    "fast-csv": "^5.0.1",
    "joi": "^17.11.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/aws-lambda": "^8.10.131",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.10.0",
    "@types/yauzl": "^2.10.3",
    "aws-sdk-client-mock": "^4.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "biome check --write --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched"
    ],
    "*.{json,md}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  }
}
